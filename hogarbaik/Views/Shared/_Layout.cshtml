<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>@ViewBag.Title</title>
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/assets/images/logo-baik.png">

    <!-- third party css -->
    <link href="~/assets/libs/datatables/dataTables.bootstrap4.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/libs/datatables/responsive.bootstrap4.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/libs/datatables/buttons.bootstrap4.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/libs/datatables/select.bootstrap4.css" rel="stylesheet" type="text/css" />
    <!-- third party css end -->
    <!-- Bootstrap Css -->
    <link href="~/assets/css/bootstrap.min.css" id="bootstrap-stylesheet" rel="stylesheet" type="text/css" />
    <!-- Icons Css -->
    <link href="~/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <!-- App Css-->
    <link href="~/assets/css/app.min.css" id="app-stylesheet" rel="stylesheet" type="text/css" />

</head>
<body>



    <!-- Begin page -->
    <div id="wrapper" >


        <!-- Topbar Start -->
        <div class="navbar-custom" >
            <ul class="list-unstyled topnav-menu float-right mb-0">



                <li class="dropdown notification-list">
                    <a class="nav-link dropdown-toggle nav-user mr-0 waves-effect" data-toggle="dropdown" href="#" role="button" aria-haspopup="false" aria-expanded="false">
                        <img src="~/assets/images/logo-baik.png" alt="user-image" class="rounded-circle">
                        <span class="pro-user-name ml-1">
                            Administrador <i class="mdi mdi-chevron-down"></i>
                        </span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right profile-dropdown ">
                        <!-- item-->
                        <div class="dropdown-header noti-title">
                            <h6 class="text-overflow m-0">Bienvenido!</h6>
                        </div>

                        <!-- item-->
                       




                        <div class="dropdown-divider"></div>

                        <!-- item-->
                        <a href="/InicioSesion/Index" class="dropdown-item notify-item">
                            <i class="fe-log-out"></i>
                            <span>Cerrar Sesión</span>
                        </a>

                    </div>
                </li>




            </ul>

            <!-- LOGO -->
            <div class="logo-box">
                <a href="index.html" class="logo logo-dark text-center">
                    <span class="logo-lg">
                       @* <img src="~/assets/images/logo-dark.png" alt="" height="16">*@
                    </span>
                    <span class="logo-sm">
                        @*<img src="~/assets/images/logo-sm.png" alt="" height="24">*@
                    </span>
                </a>
                <a href="index.html" class="logo logo-light text-center">
                    <span class="logo-lg">
                        @*<img src="~/assets/images/logo-light.png" alt="" height="16">*@
                    </span>
                    <span class="logo-sm">
                        @*<img src="~/assets/images/logo-sm.png" alt="" height="24">*@
                    </span>
                </a>
            </div>

            <ul class="list-unstyled topnav-menu topnav-menu-left mb-0">
                <li>
                    <button class="button-menu-mobile disable-btn waves-effect">
                        <i class="fe-menu"></i>
                    </button>
                </li>



            </ul>

        </div>
        <!-- end Topbar -->
        <!-- ========== Left Sidebar Start ========== -->


        <div class="left-side-menu">

            <div class="slimscroll-menu">

                <!-- User box -->
                <div class="user-box text-center">
                    <img src="~/assets/images/logo-baik.png" alt="user-img" title="Mat Helme" class="rounded-circle img-thumbnail avatar-md">
                    <div class="dropdown">
                        <a href="#" class="user-name dropdown-toggle h5 mt-2 mb-1 d-block" data-toggle="dropdown" aria-expanded="false">Administrador</a>
                        <div class="dropdown-menu user-pro-dropdown">

                            <!-- item-->
                           




                            <!-- item-->
                            <a href="/InicioSesion/Index" class="dropdown-item notify-item">
                                <i class="fe-log-out mr-1"></i>
                                <span>Cerrar Sesión</span>
                            </a>

                        </div>
                    </div>

                </div>

                <br />
                <br />
                <br />


                <!--- Menu Vertical -->
                <div id="sidebar-menu">

                    <ul class="metismenu" id="side-menu">

                        <li class="menu-title">Administración</li>

                        <li>
                            <a href="/Inicio/Inicio">
                                <i class="mdi mdi-view-dashboard"></i>
                                <span> Inicio </span>
                            </a>
                        </li>

                        <li>
                            <a href="javascript: void(0);">
                                <i class="mdi mdi-account-child"></i>
                                <span> Niños </span>
                                <span class="menu-arrow"></span>
                            </a>
                            <ul class="nav-second-level" aria-expanded="false">
                                <li><a href="/Ninos/NinosActivos" class="mdi mdi-baby">  Niños Activos</a></li>
                                <li><a href="/Ninos/NinosInactivos" class="mdi mdi-baby">  Niños Inactivos</a></li>
                                <li><a href="/Ninos/NuevoExpediente" class="mdi mdi-archive">  Nuevo Expediente</a></li>

                            </ul>
                        </li>


                        <li>
                            <a href="javascript: void(0);">
                                <i class="mdi mdi-page-layout-sidebar-left"></i>
                                <span> Inventario </span>
                                <span class="menu-arrow"></span>
                            </a>
                            <ul class="nav-second-level" aria-expanded="false">
                                <li><a href="/Inventario/InventarioAlimentacion" class="mdi mdi-baguette">  Alimentación</a></li>
                                <li><a href="/Inventario/InventarioLimpieza" class="mdi mdi-broom">  Limpieza</a></li>
                                <li><a href="/Inventario/InventarioMuebles" class="mdi mdi-car">  Muebles</a></li>
                                <li><a href="/Inventario/InventarioInmuebles" class="mdi mdi-monitor">  Inmuebles</a></li>

                            </ul>
                        </li>

                        <li>
                            <a href="javascript: void(0);">
                                <i class="mdi mdi-account-cash-outline"></i>
                                <span> Socios </span>
                                <span class="menu-arrow"></span>
                            </a>
                            <ul class="nav-second-level" aria-expanded="false">
                                <li><a href="/Socio/Solicitudes" class="mdi mdi-clipboard-check-multiple">  Solicitudes</a></li>
                                <li><a href="/Socio/SolicitudesAprobadas" class="mdi mdi-thumb-up"> Solicitudes Aprobadas</a></li>
                                <li><a href="/Socio/SolicitudesDenegadas" class="mdi mdi-thumb-down"> Solicitudes Negadas</a></li>


                            </ul>
                        </li>

                        <li>
                            <a href="javascript: void(0);">
                                <i class="fas fa-project-diagram"></i>
                                <span> Proyectos </span>
                                <span class="menu-arrow"></span>
                            </a>
                            <ul class="nav-second-level" aria-expanded="false">
                                <li><a href="/Proyectos/Proyectos" class="mdi mdi-alpha-p-box"> Ver proyectos</a></li>
                                <li><a href="/Proyectos/VerControlProyectos" class="mdi mdi-beaker-check"> Control proyectos</a></li>



                            </ul>
                        </li>

                        <li>
                            <a href="/Personal/Empleado">
                                <i class="mdi mdi-account-box-multiple"></i>
                                <span> Gestión Personal </span>
                            </a>
                        </li>
                        <li>
                            <a href="/Bitacoras/BitacoraInicio">
                                <i class="fas fa-desktop"></i>
                                <span> Bitácoras </span>
                            </a>
                        </li>

                        <li>
                            <a href="javascript: void(0);">
                                <i class="far fa-calendar-alt"></i>
                                <span> Actividades </span>
                                <span class="menu-arrow"></span>
                            </a>
                            <ul class="nav-second-level" aria-expanded="false">

                                <li><a href="/Actividades/Actividades" class="mdi mdi-book-plus-multiple"> Nueva actividad</a></li>
                                <li><a href="/Actividades/VerActividades" class="mdi mdi-border-color"> Gestionar actividad</a></li>
                            </ul>
                        </li>
                        <li>
                            <a href="/Reportes/Reportes">
                                <i class="mdi mdi-download "></i>
                                <span> Reportes </span>
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- Final Menu Vertical -->

                <div class="clearfix"></div>

            </div>
            <!-- Sidebar -left -->

        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->

        <div class="">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>



        </div>

        <!-- ============================================================== -->
        <!-- End Page content -->
        <!-- ============================================================== -->


    </div>
    <!-- END wrapper -->




    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - Sistema implementado por los estudiantes de la  Universidad Fidélitas -
        </div>
    </footer>
    <!-- Vendor js -->
    <script src="~/assets/js/vendor.min.js"></script>

    <!-- third party js -->
    <script src="~/assets/libs/datatables/jquery.dataTables.min.js"></script>
    <script src="~/assets/libs/datatables/dataTables.bootstrap4.js"></script>
    <script src="~/assets/libs/datatables/dataTables.responsive.min.js"></script>
    <script src="~/assets/libs/datatables/responsive.bootstrap4.min.js"></script>
    <script src="~/assets/libs/datatables/dataTables.buttons.min.js"></script>
    <script src="~/assets/libs/datatables/buttons.bootstrap4.min.js"></script>
    <script src="~/assets/libs/datatables/buttons.html5.min.js"></script>
    <script src="~/assets/libs/datatables/buttons.flash.min.js"></script>
    <script src="~/assets/libs/datatables/buttons.print.min.js"></script>
    <script src="~/assets/libs/datatables/dataTables.keyTable.min.js"></script>
    <script src="~/assets/libs/datatables/dataTables.select.min.js"></script>
    <script src="~/assets/libs/pdfmake/pdfmake.min.js"></script>
    <script src="~/assets/libs/pdfmake/vfs_fonts.js"></script>
    <!-- third party js ends -->
    <!-- Datatables init -->
    <script src="~/assets/js/pages/datatables.init.js"></script>

    <!-- App js -->
    <script src="~/assets/js/app.min.js"></script>

    @RenderSection("Scripts", required: false)



</body>
</html>
